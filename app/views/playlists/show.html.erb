<h2><%= @playlist.title %></h2>

<p><%= @playlist.description %></p>

<p><%= @playlist.url %></p>

<p><%= @playlist.user.email %></p>


<%= render '/reviews/review_form' %>


<% @playlist.reviews.each do |review| %>
  <div>Rating: <%= review.rating %></div>
  <div><%= review.description %></div>
  <div> <%= link_to "Edit", edit_playlist_review_path(@playlist, review)  %> </div>
<% end %>
