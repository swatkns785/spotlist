<h2><%= @playlist.title %></h2>

<p><%= @playlist.description %></p>

<p><%= @playlist.url %></p>

<p><%= @playlist.user.email %></p>


<%= render '/reviews/review_form' %>

<% if @reviews != nil %>
<% @reviews.each do |review| %>
  <%= review.rating %>
  <%= review.description %>
<% end %>
<% end %>
